    "@types/dat.gui": "0.7.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/socket.io": "2.1.2",
    "@types/socket.io-client": "1.4.32",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "2.0.2",
